{{#if resume.publications.length}}
	<section id="publications">
		<header>
			<h2>Publications</h2>
		</header>

		<ul>
			{{#each resume.publications}}
				{{#if include}}
					<li>
						{{#if url}}<a href="{{{url}}}">{{/if}}{{name}}{{#if url}}</a>{{/if}}{{#if releaseDate}} (<time datetime="{{releaseDate}}">{{onlyYear releaseDate}}</time>){{/if}}
					</li>
				{{/if}}
			{{/each}}
		</ul>
	</section>
{{/if}}
